{
    "manifest_version": 3,
    "name": "Holo Status",
    "version": "1.0",
    "description": "see livestreams",
    "permissions":[
      "https://youtube.com/*",
      "https://www.googleapis.com/*",
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "https://youtube.com/*",
      "https://www.googleapis.com/",
      "https://*/"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  